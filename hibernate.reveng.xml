		<column name="PROVINCE_LETTER" type="java.lang.String"></column>
	</table>
	<table name="YEAR" schema="ALUMNI">
		<primary-key>
			<generator class="sequence"></generator>
			<key-column name="ID" type="java.lang.Integer" />
		</primary-key>
		<column name="YEAR" type="java.lang.Integer"></column>
	</table>
	<table name="COMMENTS" schema="ALUMNI">
