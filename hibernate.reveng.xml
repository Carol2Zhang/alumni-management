	</table>
	<table name="NEWS" schema="ALUMNI">
		<primary-key>
			<generator class="sequence"></generator>
			<key-column name="ID" type="java.lang.Integer" />
		</primary-key>
		<column name="NEWS_TITLE" type="java.lang.String"></column>
		<column name="NEWS_PICTURE" type="java.lang.String"></column>
		<column name="NEWS_DESCRIPTION" type="java.lang.String"></column>
	</table>
</hibernate-reverse-engineering>