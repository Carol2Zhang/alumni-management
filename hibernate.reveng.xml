		<column name="ADMIN_NAME" type="java.lang.String"></column>
		<column name="ADMIN_PASSWORD" type="java.lang.String"></column>
	</table>
	<table name="CITY" schema="ALUMNI">
		<primary-key>
			<generator class="sequence"></generator>
			<key-column name="ID" type="java.lang.Integer" />
		</primary-key>
		<column name="CITY_NAME" type="java.lang.String"></column>
		<column name="CITY_LETTER" type="java.lang.String"></column>
