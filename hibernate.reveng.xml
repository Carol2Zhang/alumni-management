		<column name="USER_GRADE_ID" type="java.lang.Integer"></column>
		<column name="USER_CLASS_ID" type="java.lang.Integer"></column>
		<column name="USER_PROVINCE_ID" type="java.lang.Integer"></column>
		<column name="USER_CITY_ID" type="java.lang.Integer"></column>
		<column name="USER_INDUSTRY_ID" type="java.lang.Integer"></column>
		<column name="USER_NAME" type="java.lang.String"></column>
		<column name="USER_PASSWORD" type="java.lang.String"></column>
		<column name="USER_PHONE" type="java.lang.String"></column>
		<column name="USER_EMAIL" type="java.lang.String"></column>
	</table>
	<table name="CLASS" schema="ALUMNI">
		<primary-key>
			<generator class="sequence"></generator>
			<key-column name="ID" type="java.lang.Integer" />
		</primary-key>
		<column name="CLASS" type="java.lang.Integer"></column>
	</table>
	<table name="GRADE" schema="ALUMNI">
		<primary-key>
			<generator class="sequence"></generator>
			<key-column name="ID" type="java.lang.Integer" />
		</primary-key>
		<column name="GRADE" type="java.lang.String"></column>
